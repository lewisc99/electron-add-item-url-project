{
  "name": "Readit",
  "version": "0.9.0",
  "description": "Master Electron Project",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon --exec electron .",
    "reset": "git reset --hard",
    "build": "electron-builder --help",
    "release": "electron-builder -w --publish always"
  },
  "repository": "https://github.com/lewisc99/electron-add-item-url-project",
  "homepage": "https://github.com/lewisc99/electron-add-item-url-project",
  "keywords": [
    "Electron",
    "Master Electron",
    "demo"
  ],
  "author": "Lewis",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.0.0",
    "electron-builder": "^24.13.3",
    "nodemon": "^2.0.0"
  },
  "dependencies": {
    "electron-log": "^5.1.2",
    "electron-updater": "^6.1.8",
    "electron-window-state": "^5.0.3"
  },
  "build": {
    "appId": "com.lewis.readit",
    "copyright": "Copyright Stackacademy.tv",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    }
  },
  "publish": {
    "provider": "github",
    "always": true
  },
  "github": {
    "release": true
  }
}

